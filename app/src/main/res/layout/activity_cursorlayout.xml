<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CursorLayout"
    tools:layout_editor_absoluteY="25dp">

    <SeekBar
        android:id="@+id/cursorLowSpeedY_seekbar"
        android:layout_width="155dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="@+id/cursorLowSpeedX_seekbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.648"
        app:layout_constraintStart_toEndOf="@+id/cursorLowSpeedX_seekbar"
        app:layout_constraintTop_toTopOf="@+id/cursorLowSpeedX_seekbar"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/Cursor_Title"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="7dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="16dp"
        android:text="Cursor"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/cursorHighSpeedPitch_Text"
        app:layout_constraintEnd_toStartOf="@+id/cursorHighSpeed_Text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cursorLowSpeedPitch_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="38dp"
        android:layout_marginEnd="78dp"
        android:text="Pitch"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/cursorLowSpeedY_seekbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorLowSpeedX_seekbar" />

    <TextView
        android:id="@+id/cursorLowAnglePitch_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Pitch"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintStart_toEndOf="@+id/cursorAngle2_Text"
        app:layout_constraintTop_toBottomOf="@+id/cursorHighAngleY_seekbar" />

    <TextView
        android:id="@+id/cursorHighAnglePitch_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="37dp"
        android:layout_marginEnd="78dp"
        android:text="Pitch"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/cursorHighAngleY_seekbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorHighAngleX_seekbar" />

    <TextView
        android:id="@+id/cursorHighSpeedPitch_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="81dp"
        android:layout_marginEnd="77dp"
        android:layout_marginTop="83dp"
        android:text="Pitch"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/cursorLowSpeed_Text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorHighSpeedX_seekbar"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cursorLowSpeedRoll_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Roll"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintEnd_toEndOf="@+id/cursorLowSpeedX_seekbar"
        app:layout_constraintStart_toStartOf="@+id/cursorLowSpeedX_seekbar"
        app:layout_constraintTop_toBottomOf="@+id/cursorHighSpeedX_seekbar" />

    <TextView
        android:id="@+id/cursorLowAngleRoll_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginStart="84dp"
        android:text="Roll"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toTopOf="@+id/cursorLowAngleX_seekbar"
        app:layout_constraintEnd_toStartOf="@+id/cursorLowAngleY_seekbar"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cursorHighAngleRoll_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="99dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="18dp"
        android:text="Roll"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintEnd_toStartOf="@+id/cursorHighAngleY_seekbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cursorAngle_Text" />

    <TextView
        android:id="@+id/cursorHighSpeedRoll_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="98dp"
        android:layout_marginStart="86dp"
        android:text="Roll"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toTopOf="@+id/cursorHighSpeedX_seekbar"
        app:layout_constraintEnd_toStartOf="@+id/cursorHighSpeedY_seekbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cursorHighSpeedX_seekbar" />

    <TextView
        android:id="@+id/cursorHighSpeed_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="157dp"
        android:text="High speed"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/Cursor_Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Cursor_Title" />

    <TextView
        android:id="@+id/cursorLowSpeed_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="164dp"
        android:text="Low speed"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cursorAngle2_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="Low speed activation angle"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/cursorLowAnglePitch_Text"
        app:layout_constraintStart_toEndOf="@+id/cursorLowAngleRoll_Text" />

    <TextView
        android:id="@+id/cursorAngle_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        android:layout_marginStart="22dp"
        android:text="High speed activation angle"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/cursorHighAnglePitch_Text"
        app:layout_constraintStart_toStartOf="@+id/cursorHighAngleRoll_Text" />

    <TextView
        android:id="@+id/cursorIcon_Title"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginBottom="97dp"
        android:layout_marginEnd="4dp"
        android:text="Cursor Icon"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/circle_button"
        app:layout_constraintStart_toStartOf="@+id/dot_button" />

    <SeekBar
        android:id="@+id/cursorLowSpeedX_seekbar"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginBottom="103dp"
        android:layout_marginEnd="91dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="103dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="@+id/cursorHighAngleX_seekbar"
        app:layout_constraintEnd_toStartOf="@+id/cursorLowSpeedPitch_Text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cursorHighSpeedX_seekbar" />

    <SeekBar
        android:id="@+id/cursorHighSpeedX_seekbar"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginBottom="28dp"
        android:layout_marginEnd="93dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="31dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toTopOf="@+id/cursorLowSpeed_Text"
        app:layout_constraintEnd_toStartOf="@+id/cursorHighSpeedPitch_Text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cursor_Title" />

    <SeekBar
        android:id="@+id/cursorLowAngleY_seekbar"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="15dp"
        android:max="90"
        android:progress="45"
        app:layout_constraintBottom_toTopOf="@+id/cursorIcon_Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorLowAngleRoll_Text"
        app:layout_constraintTop_toTopOf="@+id/cursorLowAnglePitch_Text" />

    <SeekBar
        android:id="@+id/cursorHighAngleY_seekbar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="13dp"
        android:max="90"
        android:progress="45"
        app:layout_constraintBottom_toBottomOf="@+id/cursorHighAngleX_seekbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorHighAngleRoll_Text"
        app:layout_constraintTop_toTopOf="@+id/cursorHighAngleX_seekbar" />

    <SeekBar
        android:id="@+id/cursorHighSpeedY_seekbar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="13dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="@+id/cursorHighSpeedX_seekbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cursorHighSpeedRoll_Text"
        app:layout_constraintTop_toTopOf="@+id/cursorHighSpeedX_seekbar" />

    <SeekBar
        android:id="@+id/cursorLowAngleX_seekbar"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginBottom="37dp"
        android:layout_marginEnd="1dp"
        android:layout_marginTop="40dp"
        android:max="90"
        android:progress="45"
        app:layout_constraintBottom_toBottomOf="@+id/cursorIcon_Title"
        app:layout_constraintEnd_toEndOf="@+id/cursorHighAngleX_seekbar"
        app:layout_constraintStart_toStartOf="@+id/cursorIcon_Title"
        app:layout_constraintTop_toTopOf="@+id/cursorAngle2_Text" />

    <SeekBar
        android:id="@+id/cursorHighAngleX_seekbar"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginBottom="127dp"
        android:layout_marginEnd="94dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="127dp"
        android:max="90"
        android:progress="45"
        app:layout_constraintBottom_toBottomOf="@+id/cursorIcon_Title"
        app:layout_constraintEnd_toStartOf="@+id/cursorHighAnglePitch_Text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cursorLowSpeedRoll_Text" />

    <ImageButton
        android:id="@+id/dot_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="7dp"
        android:layout_marginEnd="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp"
        android:contentDescription="todo"
        android:src="@drawable/reddot"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/circle_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cursorIcon_Title"
        tools:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cursor_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="1dp"
        android:contentDescription="todo"
        android:src="@drawable/mouse"
        app:layout_constraintBottom_toBottomOf="@+id/circle_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circle_button"
        app:layout_constraintTop_toTopOf="@+id/circle_button"
        tools:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/circle_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="3dp"
        android:layout_marginEnd="38dp"
        android:contentDescription="todo"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cursor_button"
        app:layout_constraintStart_toEndOf="@+id/dot_button"
        app:layout_constraintTop_toTopOf="@+id/dot_button"
        tools:background="@android:color/transparent" />

</android.support.constraint.ConstraintLayout>